<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>Controlador.ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCaja</servlet-name>
        <servlet-class>Controlador.ServletCaja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvUsuario</servlet-name>
        <servlet-class>Controlador.SvUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvUsuarios</servlet-name>
        <servlet-class>Controlador.SvUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletAlimento</servlet-name>
        <servlet-class>Controlador.ServletAlimento</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/ServletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCaja</servlet-name>
        <url-pattern>/ServletCaja</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvUsuario</servlet-name>
        <url-pattern>/SvUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvUsuarios</servlet-name>
        <url-pattern>/SvUsuarios</url-pattern>
    </servlet-mapping>
    <dependency>
        <groupId>commons-fileupload</groupId>
        <artifactId>commons-fileupload</artifactId>
        <version>1.4</version>
    </dependency>
    <servlet-mapping>
        <servlet-name>ServletAlimento</servlet-name>
        <url-pattern>/ServletAlimento</url-pattern>
    </servlet-mapping>
</web-app>
